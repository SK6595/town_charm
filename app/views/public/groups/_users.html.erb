<table>
  <thead>
    <tr>
      <th>name</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td>
          <% if group.owner_id == current_user.id %>
            <%= render 'public/group_users/form', group_user: user.group_users.find_by(group: group) %>
          <% end %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>